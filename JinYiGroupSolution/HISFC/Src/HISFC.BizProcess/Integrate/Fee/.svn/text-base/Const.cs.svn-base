            hashTable.Add(INTERFACE_POP_FEE, "Neusoft.HISFC.BizProcess.Integrate.FeeInterface.IOutpatientPopupFee");
            hashTable.Add(INTERFACE_CHOOSE_ITEM, "Neusoft.HISFC.BizProcess.Integrate.FeeInterface.IChooseItemForOutpatient");

            if (hashTable.ContainsKey(constName))
            {
                return hashTable[constName].ToString();
